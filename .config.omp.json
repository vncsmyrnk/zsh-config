{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "transient_prompt": {
    "background": "transparent",
    "foreground": "#ffffff",
    "template": "‚ùØ "
  },
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "lightYellow",
          "style": "plain",
          "template": "{{ .UserName }} ",
          "type": "session"
        },
        {
          "foreground": "#4988C4",
          "options": {
            "style": "folder"
          },
          "style": "plain",
          "template": "<#ffffff>in</> {{ if not .Writable}}üîí{{ end }}{{ .Path }} ",
          "type": "path"
        },
        {
          "foreground": "#75B06F",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#FFCF50{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#FFCC80{{ end }}",
            "{{ if gt .Ahead 0 }}#B388FF{{ end }}",
            "{{ if gt .Behind 0 }}#E9D484{{ end }}"
          ],
          "options": {
            "fetch_status": true,
            "source": "cli"
          },
          "style": "plain",
          "template": "<#ffffff>on</> {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} ÔÅÑ {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} ÔÅÜ {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} ÔÉá {{ .StashCount }}{{ end }} ",
          "type": "git"
        },
        {
          "type": "go",
          "style": "plain",
          "foreground": "#7FD5EA",
          "template": " Óòß {{ .Full }} "
        },
        {
          "type": "kubectl",
          "style": "plain",
          "foreground": "#ebcc34",
          "template": " Û±Éæ {{.Context}}{{if .Namespace}} :: {{.Namespace}}{{end}} "
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "type": "status",
          "style": "plain",
          "foreground": "#ffffff",
          "template": "{{ if .Error }}({{ .String }}){{ else }}{{ end }}"
        },
        {
          "type": "executiontime",
          "style": "plain",
          "foreground": "#ffffff",
          "template": " {{ .FormattedMs }}",
          "options": {
            "threshold": 500,
            "style": "austin"
          }
        },
        {
          "type": "time",
          "style": "plain",
          "foreground": "#ffffff",
          "template": " {{ .CurrentDate | date .Format }}",
          "options": {
            "time_format": "15:04:05"
          }
        },
        {
          "type": "session",
          "style": "diamond",
          "foreground": "#ffffff",
          "background": "#E37434",
          "leading_diamond": "ÓÇ∂",
          "trailing_diamond": "ÓÇ¥ ",
          "template": "{{ if .SSHSession }}ÓÆ© {{ .UserName }}{{ end }}"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "lightGreen",
          "style": "plain",
          "foreground_templates": [
            "{{ if .Segments.Status.Error }}#e91e63{{ end }}"
          ],
          "template": "‚ùØ",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "async": true,
  "version": 4
}
